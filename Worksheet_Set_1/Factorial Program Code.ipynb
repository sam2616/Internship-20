   ],
   "source": [
    "num=int(input(\"Enter the number: \"))\n",
    "if num<0:\n",
    "    print(\"Factorial does not exist for negative numbers\")\n",
    "elif num==0:\n",
    "    print(\"The factorial of 0 is 1\")\n",
    "else:\n",
    "    fact=1\n",
    "    i=1\n",
    "    while i<num:\n",
    "        fact=fact*(i+1)\n",
    "        i=i+1\n",
    "    print(\"The Factorial of\",num,\"is\",fact)"
   ]
